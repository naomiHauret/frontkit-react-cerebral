{
	"extends": [
		"standard",
		"plugin:react/recommended",
		"prettier",
		"prettier/react",
        "prettier/standard",
        "plugin:jsx-a11y/recommended"
	],
	"plugins": ["jsx-a11y","react", "prettier", "standard"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"node": true,
		"jest": true
	},
	"rules": {
		"prettier/prettier": "error"
	}
}
